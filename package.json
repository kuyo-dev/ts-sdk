{
  "name": "@kuyo/sdk",
  "version": "0.1.0",
  "description": "Simple error tracking SDK for Next.js apps",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kuyo-dev/ts-sdk"
  },
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:types && rollup -c && npm run build:check",
    "build:types": "cd ../types && npm run build",
    "build:check": "node -e \"console.log('âœ… Build completed:', require('./dist/index.js'))\"",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "typecheck": "npm run build:types && tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "error-tracking",
    "nextjs",
    "bug-reporting",
    "monitoring"
  ],
  "author": "Kuyo Team",
  "license": "MIT",
  "dependencies": {
    "@kuyo/types": "*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rollup": "^3.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "typescript": "^5.0.0",
    "tslib": "^2.6.0",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  }
}